#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules/shadcn-ui/dist/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules/shadcn-ui/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules/shadcn-ui/dist/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules/shadcn-ui/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/shadcn-ui@0.4.1_typescript@5.3.2/node_modules:/Users/minookim/Desktop/fullstack_javascript/homework-7-2-flashcards-Minoo-Kim/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shadcn-ui/dist/index.js" "$@"
else
  exec node  "$basedir/../shadcn-ui/dist/index.js" "$@"
fi
